<body class="text-center">

    <div id="header">
         <div id="logIn"></div>
        <button id="logout">LOGOUT</button>
    </div>
       

    <div id="main"></div>
    <button id="button">Add TimeSlot</button>
    <div id="modal" class="modal">

        <div class="modal-content">
            <p>Confirm Time Slot?</p>
            <label>Date: (YYYY-MM-DD)</label><input type="text" id="date">
            <label>Start Time: (HH:MM)</label><input type="text" id="start">
            <label>End Time: (HH:MM)</label><input type="text" id="end">
            
            <button class="close">Cancel</button>
            <button id="confirmTime">Confirm Time</button>
        </div>

    </div>

    <div id="modal2" class="modal">
        <div class="modal-content">
            <p>Are you sure you want to delete this time slot?</p>
            <label>Start Time: (HH:MM)</label><span id="confirmST"></span>
            <label>End Time: (HH:MM)</label><span id="confirmET"></span>
            
            <button class="close2">Cancel</button>
            <button id="confirmDelete">Confirm Delete</button>
        </div>

    </div>
    <script src = "/javascript/script.js"></script>
</body>